@prefix foaf:<http://xmlns.com/foaf/0.1/> .
<http://example.org/person/fadi> foaf:name "Fadi";
	<http://example.org/property/hobby> "football";
	<http://xmlns.com/foaf/0.1/nick> "sheer";
	a foaf:Person
	.

<http://example.org/person/gofran> foaf:name "Gofran";
	foaf:member <http://example.org/organisation/deri> ;
