SELECT (COUNT(DISTINCT ?s) AS ?count) WHERE{{SERVICE <http://localhost:3030/test/query>{?s a <http://www.w3.org/ns/dcat#Dataset> .}}UNION{SERVICE <http://localhost:3031/test/query>{?s a <http://www.w3.org/ns/dcat#Dataset> .}}{SERVICE <http://localhost:3030/test/query>{{?s <http://www.w3.org/ns/dcat#keyword> ?tag. FILTER(str(?tag)="opendata") .}UNION{?s <http://www.w3.org/ns/dcat#keyword> ?tag. FILTER(str(?tag)="format-rdfa") .}UNION{?s <http://www.w3.org/ns/dcat#keyword> ?tag. FILTER(str(?tag)="format-rdf") .}UNION{?s <http://www.w3.org/ns/dcat#keyword> ?tag. FILTER(str(?tag)="ontology") .}}}UNION{SERVICE <http://localhost:3031/test/query>{{?s <http://www.w3.org/ns/dcat#keyword> ?tag. FILTER(str(?tag)="opendata") .}UNION{?s <http://www.w3.org/ns/dcat#keyword> ?tag. FILTER(str(?tag)="format-rdfa") .}UNION{?s <http://www.w3.org/ns/dcat#keyword> ?tag. FILTER(str(?tag)="format-rdf") .}UNION{?s <http://www.w3.org/ns/dcat#keyword> ?tag. FILTER(str(?tag)="ontology") .}}}OPTIONAL {{SERVICE <http://localhost:3030/test/query>{{?s <http://purl.org/dc/terms/creator> ?creator .}}}UNION{SERVICE <http://localhost:3031/test/query>{{?s <http://purl.org/dc/terms/creator> ?creator .}}}} FILTER(!bound(?creator)) .} GROUP BY ?creator
